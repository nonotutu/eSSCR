    %dl
      %dt Starts_at :
      %dd= @service.starts_at.to_s(:cust_long)
    %dl
      %dt Ends_at :
      %dd= @service.ends_at.to_s(:cust_long)
    %dl
      %dt Rendezvous :
      %dd= @service.rendezvous
    %dl
      %dt Depart_at :
      %dd= @service.depart_at
    %dl
      %dt Surplace_at :
      %dd= @service.surplace_at